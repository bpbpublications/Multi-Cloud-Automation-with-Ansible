- name: Configure Qradar Network Settings\
  hosts: qradar\
  become: yes\
  tasks:\
    - name: Set network interface settings for eth0\
      ibm.qradar.network_interface_module:  # Replace with the actual module name for interface configuration\
        interface: eth0\
        ip_address: 10.0.0.1\
        subnet_mask: 255.255.255.0\
